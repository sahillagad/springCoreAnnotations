package com.masai;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service 
public class A {

	
	@Autowired
	private B b;
	
	public void showA() {
	
		System.out.println("I AM INSIDE THE SHOWA() METHOD A CLASS");
	    System.out.println("Address OF  the B "+b); 	
		
	}
	
	
	
}
