package com.masai;

import org.springframework.stereotype.Service;

import com.masai1.C;

@Service
public class A {

 private B b;
 private C c;
 
 
 
  public void showA() {
	 System.out.println("I am inside the A class"); 
	 System.out.println("B Object : "+b);
	 System.out.println("C Object : "+c);
 
    System.out.println("------------------------------");	  
  }

  
  
	
	
	
}
